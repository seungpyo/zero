import math;

print_curve = (string s, func f, int from, int to) -> none {
    println(s);
    range(from, to) -> (int x) {
        y = f(x);
        println(" " * (y - 1) + "*");
    };
};

run = () -> exitcode {
    sin_func = (int x) -> int {
        xf = float(x);
        yf = sin(x);
        y = int(yf);
        return y;
    };
    print_curve(
        s: "This is my sin curve",
        f: sin_func,
        from: 0,
        to: 10,
    );
};